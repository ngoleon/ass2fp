            <Label text="First Name"/>
            <TextField fx:id="fNameField"/>
            <Label fx:id="welcomeText1" text="Last Name"/>
            <TextField fx:id="lNameField"/>
            <Label text="Password"/>
            <TextField fx:id="passField"/>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="51.0" layoutY="331.0" prefHeight="58.0" prefWidth="147.0" spacing="10.0">
